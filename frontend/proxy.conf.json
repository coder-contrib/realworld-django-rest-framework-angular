{
  "/api": {
    "target": "http://localhost:8000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "info",
    "headers": {
      "Connection": "keep-alive"
    }
  },
  "/api/*": {
    "target": "http://localhost:8000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "info",
    "headers": {
      "Connection": "keep-alive"
    }
  }
}